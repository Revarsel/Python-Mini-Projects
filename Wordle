                inpt2 = empty[2]
            except:
                pass
            try:
                inpt3 = empty[3]
            except:
                pass
            try:
                inpt4 = empty[4]
            except:
                pass

            for i in empty:
                if copy1 == i:
                    pos1 = "CL"
                    empty.remove(i)
                if copy2 == i:
                    pos2 = "CL"
                    empty.remove(i)
                if copy3 == i:
                    pos3 = "CL"
                    empty.remove(i)
                if copy4 == i:
                    pos4 = "CL"
                    empty.remove(i)
                if copy5 == i:
                    pos5 = "CL"
                    empty.remove(i)

            if aa == for_letter_list[0]:
                pos1 = "CP"
            if ab == for_letter_list[1]:
                pos2 = "CP"
            if ac == for_letter_list[2]:
                pos3 = "CP"
            if ad == for_letter_list[3]:
                pos4 = "CP"
            if ae == for_letter_list[4]:
                pos5 = "CP"

            if pos1 == "CL":
                color1 = orange
            if pos1 == "CP":
                color1 = green
            if pos1 == "G":
                color1 = grey

            if pos2 == "CL":
                color2 = orange
            elif pos2 == "CP":
                color2 = green
            elif pos2 == "G":
                color2 = grey

            if pos3 == "G":
                color3 = grey
            elif pos3 == "CP":
                color3 = green
            elif pos3 == "CL":
                color3 = orange

            if pos4 == "CL":
                color4 = orange
            elif pos4 == "CP":
                color4 = green
            elif pos4 == "G":
                color4 = grey

            if pos5 == "CL":
                color5 = orange
            elif pos5 == "CP":
                color5 = green
            elif pos5 == "G":
                color5 = grey

            if rounds == 0:
                position1_color = color1
                position2_color = color2
                position3_color = color3
                position4_color = color4
                position5_color = color5

            elif rounds == 1:
                position6_color = color1
                position7_color = color2
                position8_color = color3
                position9_color = color4
                position10_color = color5

            elif rounds == 2:
                position11_color = color1
                position12_color = color2
                position13_color = color3
                position14_color = color4
                position15_color = color5

            elif rounds == 3:
                position16_color = color1
                position17_color = color2
                position18_color = color3
                position19_color = color4
                position20_color = color5

            elif rounds == 4:
                position21_color = color1
                position22_color = color2
                position23_color = color3
                position24_color = color4
                position25_color = color5

            elif rounds == 5:
                position26_color = color1
                position27_color = color2
                position28_color = color3
                position29_color = color4
                position30_color = color5

            print("Loading...")
            time.sleep(1)

            if rounds >= 0:
                letters(black, position1, position2, position3, position4, position5, position1_color, position2_color, position3_color, position4_color, position5_color)

            if rounds >= 1:
                letters(black, position6, position7, position8, position9, position10, position6_color, position7_color, position8_color, position9_color, position10_color)

            if rounds >= 2:
                letters(black, position11, position12, position13, position14, position15, position11_color, position12_color, position13_color, position14_color, position15_color)

            if rounds >= 3:
                letters(black, position16, position17, position18, position19, position20, position16_color, position17_color, position18_color, position19_color, position20_color)

            if rounds >= 4:
                letters(black, position21, position22, position23, position24, position25, position21_color, position22_color, position23_color, position24_color, position25_color)

            if rounds >= 5:
                letters(black, position26, position27, position28, position29, position30, position26_color, position27_color, position28_color, position29_color, position30_color)

            if rounds == 6:
                print(black, "╔═════╦═════╦═════╦═════╦═════╗", colors.reset)
                print(black, "║", color1, var1, black, "║", color2, var2, black,  "║", color3,  var3, black, "║", color4,  var4, black, "║", color5,  var5, black, "║", colors.reset)
                print(black, "╚═════╩═════╩═════╩═════╩═════╝", colors.reset)

            for i in range(1, roounds):
                print(black, "╔═════╦═════╦═════╦═════╦═════╗", colors.reset)
                print(black, "║", "   ", "║", "   ", "║", "   ", "║", "   ",
                       "║", "   ", "║", colors.reset)
                print(black, "╚═════╩═════╩═════╩═════╩═════╝", colors.reset)

            print("")
            time.sleep(1)

            if inpt == rand_text:
                print("You won!")
                take = input("Do you want to play again? (y/n) ")
                while True:
                    if take == "y":
                        yn = "y"
                        print("Loading...")
                        time.sleep(1)
                        break
                    elif take == "n":
                        yn = "n"
                        print("Exiting...")
                        time.sleep(1.5)
                        break
                    else:
                        take = input("Do you want to play again? (y/n) ")
                break
            else:
                #print("Try again.")
                pass

        elif rounds == 6:
            print("Out of rounds!")
            print("The word was ", rand_text)
            take = input("Do you want to play again? (y/n) ")
            while True:
                if take == "y":
                    yn = "y"
                    print("Loading...")
                    time.sleep(1)
                    break
                elif take == "n":
                    yn = "n"
                    print("Exiting...")
                    time.sleep(1.5)
                    break
                else:
                    take = input("Do you want to play again?(y/n) ")

