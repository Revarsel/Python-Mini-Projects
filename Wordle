import random
import time


def letters(black, var1, var2, var3, var4, var5, color1, color2, color3, color4, color5):
    print(black, "╔═════╦═════╦═════╦═════╦═════╗", colors.reset)
    print(black, "║", color1, var1, black, "║", color2, var2, black, "║", color3, var3, black, "║", color4, var4, black,
          "║", color5, var5, black, "║", colors.reset)
    print(black, "╚═════╩═════╩═════╩═════╩═════╝", colors.reset)


class colors:
    reset = f'\033[0m'
    bold = f'\033[01m'
    disable = f'\033[02m'
    underline = f'\033[04m'
    reverse = f'\033[07m'
    strikethrough = f'\033[09m'
    invisible = f'\033[08m'

    grey = f'\033[48;5;243m'
    orange = f'\033[48;5;202m'
    green = f'\033[48;5;22m'


grey = f'\033[48;5;243m'
orange = f'\033[48;5;130m'
green = f'\033[48;5;22m'
black = f'\033[48;5;238m'

print("If the color of letter is", grey, "grey", f'\033[0m', "it is the wrong letter.")
print("If the letter is", orange, "orange", f'\033[0m', "it means the letter is present in the word but has the wrong position.")
print("If the letter is", green, "green", f'\033[0m', "then the letter is in correct position and is in the word.")


